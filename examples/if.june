extern "puts" as puts(s: [char]);
extern "gets" as gets(s: [char]);
extern "atoi" as strToInt(str: [char]): int;

var ageStr: [char;10];

puts("Enter your age:");
gets(ageStr);

var age = strToInt(ageStr);

if (age < 18) [
    puts("You are not an adult :(");
] else [
    puts("You are an adult :)");
]